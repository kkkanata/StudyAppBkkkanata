<UserControl x:Class="studyApp.Views.SubView.ResultUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:studyApp.Views.SubView"
             mc:Ignorable="d" 
             d:DesignHeight="46" d:DesignWidth="800">
    <Grid>
        <Label x:Name="explanationStatementText" Content="{Binding Result_Explanation,RelativeSource={RelativeSource FindAncestor,AncestorType={x:Type local:ResultUserControl}}}" FontSize="30"/>


    </Grid>
</UserControl>
